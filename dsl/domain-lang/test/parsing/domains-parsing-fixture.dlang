group test {
    domain Ordering {
        description: "Ordering domain"
        vision: "Impeccable one-click ordering process"
    }

    domain SupplyChain {
        description: "Supply chain domain"
        vision: "Timely sourcing of relevant products"
    }

    domain Orders in Ordering {
        description: "Orders subdomain"
        vision: "Orders vision"
    }

    domainmap FulfilmentMap {
        contains Ordering, SupplyChain
    }
}